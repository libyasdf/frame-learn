<?xml version="1.0" encoding="UTF-8"?>
<project name="tjxm" default="default">
	<property name="export.dir.root" value="D:\导出项目文件\09.21.Class"></property>
	
	<target name="mqx">
		<mkdir dir="${export.dir.root}\tjxm"/>
		<copy todir="${export.dir.root}\tjxm" overwrite="true">
			<fileset refid="fengchao"/>
		</copy>
	</target>
	
	
	<fileset id="fengchao" dir="../../../../">
			
		<!--值班管理 后台class-->
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/xxkh/test/"/>

		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/xxkh/testmanage/"/>

		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/xxkh/paper/"/>

		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/xxkh/questiongroup/"/>

		<!--根据业务id删除待办-->
		<include name="build/classes/com/sinosoft/sinoep/waitNoflow/services/SysWatiNoflowServiceImpl.class"/>
		<!--学习考核-参加考试的前台页面 -->
		<include name="WebContent/modules/zhbg/xxkh/test/css/"/>
		<include name="WebContent/modules/zhbg/xxkh/test/fonts/"/>
		<include name="WebContent/modules/zhbg/xxkh/test/images/"/>
		<include name="WebContent/modules/zhbg/xxkh/test/js/"/>
		<include name="WebContent/modules/zhbg/xxkh/test/beginTest.html"/>
		<include name="WebContent/modules/zhbg/xxkh/test/testResult.html"/>

		<include name="WebContent/modules/zhbg/xxkh/testManage/css/style.css"/>
		<include name="WebContent/modules/zhbg/xxkh/testManage/ksglEnterTest.html"/>

	</fileset>
	
	<target name="all">
			<mkdir dir="${export.dir.root}/cqqx"/>
			<copy todir="${export.dir.root}/cqqx" overwrite="true">
				<fileset refid="fengchao"/>
			</copy>
		</target>
		<!--删除上次导出文件-->
		<target name="deleteOldFile" >  
		    <delete verbose="true" includeemptydirs="true" failonerror="false" >
		       <fileset dir="${export.dir.root}">
		           <include name="**/*"/>
		       </fileset>
		    </delete>
		</target> 

		<target name="default" depends="deleteOldFile,mqx">
	        <exec executable="cmd" failonerror="false">  
	            <arg line='/c explorer "${export.dir.root}\"' />  
	        </exec>
		</target>	
	</project>