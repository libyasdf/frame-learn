<body class="formpage_bj">
<div class="container-fluid formpage_area">
    <form class="form-horizontal" id="form">
        <input type="hidden" id="textId"/>
        <input name="resId" id="resId" type="hidden" value="">
        <!-- 流程字段-->
        <input name="workItemId1" id="workItemId1:" type="hidden" value=""/><!-- 操作方法 -->
        <!-- 基本信息 -->
        <div class="row">
            <div class="col-md-12 ">
                <div class="block_title">
                    <span class="name">编研信息</span>
                </div>
            </div>
        </div>
        <div class="row m-t-15">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="" class="col-md-2 control-label">文件标题：</label>
                    <div class="col-md-8 form-view">
                        <span name="title"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="col-md-2 control-label">年份：</label>
                    <div class="col-md-4 form-view">
                        <span name="year"></span>
                    </div>
                </div>
            </div>
        </div>
        <!-- 基本信息结束 -->
        <!-- 原文信息 -->
        <div class="row">
            <div class="col-md-12 ">
                <div class="block_title">
                    <span class="name">原文信息</span>
                </div>
            </div>
        </div>
        <div class="row m-t-15" >
            <div class="form-group">
                <label for="" class="col-md-2 control-label">原文：</label>
                <div class="col-md-10">
                    <!--文件上传-->
                    <div class="files row" id="p_affix">

                    </div>
                </div>
            </div>
        </div>
        <!-- 原文信息结束 -->
        <!-- 反馈信息 -->
        <div class="row">
            <div class="col-md-12 ">
                <div class="block_title">
                    <span class="name">反馈信息</span>
                </div>
            </div>
        </div>
        <div class="row m-t-15">
            <div class="form-group">
                <label for="" class="col-md-2 control-label"><b style="color: red;">*</b> 附件：</label>
                <div class="col-md-10" id="s_affix_button">
                </div>
            </div>
            <div class="form-group">
                <label for="" class="col-md-2 control-label"></label>
                <div class="col-md-10">
                    <div class="files row" id="s_affix">
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>

<!-- 按钮区 -->
<div class="form_btn_area" style="text-align: center;">
    <button id="sendFLow" class="btn btn-primary form_btn_area_btn2" onclick="fankui();" >
        <span class="glyphicon glyphicon-send" aria-hidden="true"></span> 反馈
    </button>
    <button id="close" class="btn btn-primary form_btn_area_btn2" onclick="closeForm();" >
        <span class="glyphicon glyphicon-remove-circle" aria-hidden="true"></span> 关闭
    </button>
</div>
