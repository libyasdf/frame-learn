<script type="text/javascript">
    /*laydate.render({
        elem: '#rewardTime' //指定元素
        ,done: function(value, date, endDate){
            $('#rewardTime').val(value).change()
        }
    });
    laydate.render({
        elem: '#rewardRevokeTime' //指定元素
        ,done: function(value, date, endDate){
            $('#rewardRevokeTime').val(value).change()
        }
    });*/

    var rewardTime =laydate.render({
        elem: '#rewardTime' //指定元素
        ,type:'date'
        ,done: function(value, datas){
            $('#rewardTime').val(value).change();
            rewardRevokeTime.config.min ={
                year:datas.year,
                month:datas.month-1,
                date:datas.date,
                hours:0,
                minutes:0,
                seconds:0
            }
        }
    });

    var rewardRevokeTime = laydate.render({
        elem: '#rewardRevokeTime' //指定元素
        ,done: function(value, datas){
            $('#rewardRevokeTime').val(value).change()
            rewardTime.config.max ={
                year:datas.year,
                month:datas.month-1,
                date:datas.date,
                hours:0,
                minutes:0,
                seconds:0
            }
        }
    });

    $(function () {
        $('.nav-tabs > li').unbind('click').bind('click',function(){
            var id = $(this).attr('for');
            $(this).addClass('active').siblings('li').removeClass('active');
            $('.main').children().addClass('hidden');
            $('#'+id).removeClass('hidden');
        });
    });

</script>
