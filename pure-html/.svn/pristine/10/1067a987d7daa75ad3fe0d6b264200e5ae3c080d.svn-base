              <!--  <input name="partyMember" id="partyMember" type="hidden" value=""-->
                <div class="container-fluid formpage_area" style="margin-bottom: 140px">
                    <!-- 基本信息 -->
                    <div class="row">
                        <div class="col-md-12 ">
                            <div class="block_title">
                                <span class="name">党员奖励</span>
                            </div>
                        </div>
                    </div>
                    <div class="row m-t-15">
                        <div class="col-md-12">
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"><b style="color: red;"></b> 党员姓名：</label>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control"  id="partyMember" name="partyMember" value="" readonly />
                                    </div>
                                </div>
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"><b style="color: red;"></b> 党组织：</label>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control"  id="partyOrganization" name="partyOrganization" value="" readonly/>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"><b style="color: red;">*</b> 奖励日期：</label>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control" id="rewardTime" name="rewardTime" value="">
                                    </div>
                                </div>
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"><b style="color: red;"></b> 奖励机构：</label>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control"  id="rewardUnit" name="rewardUnit" value="">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"><b style="color: red;"></b> 奖励内容：</label>
                                    <div class="col-md-4">
                                        <select class="form-control"  id="rewardContent" name="rewardContent" >
                                        </select>
                                    </div>
                                </div>
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"><b style="color: red;"></b> 奖励原因：</label>
                                    <div class="col-md-4">
                                        <select class="form-control" id="rewardReason" name="rewardReason" >
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"><b style="color: red;"></b> 批准机关级别：</label>
                                    <div class="col-md-4">
                                        <select class="form-control" id="approvalOrganLevel" name="approvalOrganLevel" >
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"><b style="color: red;"></b> 主要事迹：</label>
                                    <div class="col-md-10">
                                        <textarea type="text" class="form-control"  id="mainDeeds" name="mainDeeds" value=""></textarea>
                                    </div>
                                </div>

                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"><b style="color: red;"></b> 奖励撤销日期：</label>
                                    <div class="col-md-4">
                                        <input type="text" class="form-control" id="rewardRevokeTime" name="rewardRevokeTime"  value=""></input>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"><b style="color: red;"></b> 备注：</label>
                                    <div class="col-md-10">
                                        <textarea type="text" class="form-control"  id="remarks" name="remarks" value=""></textarea>
                                    </div>
                                </div>
                            </div>

                    </div>
                    <!-- 基本信息结束 -->
                </div>
                </div>
                </form>
