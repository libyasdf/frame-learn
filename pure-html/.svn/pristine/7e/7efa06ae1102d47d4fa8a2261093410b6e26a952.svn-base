<script type="text/javascript">
    //参考：http://www.layui.com/laydate/
    //绑定日期输入input
    laydate.render({
        elem: '#annual'
        ,type: 'year'
        ,done: function(value, date, endDate){
            $("#annual").val(value);
            var month = $("#monthval").val()
            edit($("#partyId").val(),value,month);
        }

    });
    function changeFin(msgVal,value){
        var json = {};
        var valid ,msg;
        if(value!=null&&value.length!=0){
           var array = value.split(".");
            if(array.length==2){
                value =array[1];
                if(value.length>3){
                    valid = false;
                    msg = msgVal+"交纳党费金额小数位长度不能超过3位！";
                }else{
                    valid = true;
                }
            }else{
                valid = true;
            }

        }else{
            valid = false;
            msg = msgVal+"交纳党费金额不能为空！";
        }
        json.valid = valid;
        json.message = msg;
        return json;
    }

    function myFunction(){
        edit($("#partyId").val(), $("#annual").val(), $("#monthval").val());
    }
</script>

