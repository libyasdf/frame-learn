<?xml version="1.0" encoding="UTF-8"?>
<project name="tjxm" default="default">
	<property name="export.dir.root" value="D:\导出项目文件\9.10"></property>
	
	<target name="mqx">
		<mkdir dir="${export.dir.root}\tjxm"/>
		<copy todir="${export.dir.root}\tjxm" overwrite="true">
			<fileset refid="fengchao"/>
		</copy>
	</target>
	
	
	<fileset id="fengchao" dir="../../../../../">
	       
		    <!--后勤管理——》工资管理 (bug31848，增加10个数据项)-->
			<include name="WebContent/modules/zhbg/salary/salaryList.html"/>
		    <include name="WebContent/modules/zhbg/salary/mySalaryList.html"/>
		       <include name="WebContent/modules/zhbg/salary/salaryEditForm.html"/>
		    	<include name="WebContent/modules/zhbg/salary/salaryReadOnlyForm.html"/>
		    <include name="WebContent/WEB-INF/classes/com/sinosoft/sinoep/modules/zhbg/salary/services/SalaryServiceImp.class"/>
	        <include name="WebContent/WEB-INF/classes/com/sinosoft/sinoep/modules/zhbg/salary/entity/Salary.class"/>
		    
			
	</fileset>
	
	<target name="all">
			<mkdir dir="${export.dir.root}/cqqx"/>
			<copy todir="${export.dir.root}/cqqx" overwrite="true">
				<fileset refid="fc"/>
			</copy>
		</target>
		<!--删除上次导出文件-->
		<target name="deleteOldFile" >  
		    <delete verbose="true" includeemptydirs="true" failonerror="false" >
		       <fileset dir="${export.dir.root}">
		           <include name="**/*"/>
		       </fileset>
		    </delete>
		</target> 

		<target name="default" depends="deleteOldFile,mqx">
	        <exec executable="cmd" failonerror="false">  
	            <arg line='/c explorer "${export.dir.root}\"' />  
	        </exec>
		</target>	
	</project>