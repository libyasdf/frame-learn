<script type="text/javascript">
function aip_init(){
	var obj=document.getElementById("HWPostil1");
	obj.ShowDefMenu=0;//隐藏菜单
	obj.ShowToolBar=0;//隐藏工具栏
	obj.ShowScrollBarButton=1;//隐藏滚动条的工具栏
	obj.CurrPenColor=0;//设置笔迹颜色，0是黑色，255是红色
	obj.CurrPenWidth=3;//设置笔迹宽度。1-28，-1是弹出框用户选择
	obj.CloseDoc(0);
	obj.LoadFile("./dyzmxOnly.aip");//加载aip模板
}
</script>
<SCRIPT LANGUAGE=javascript FOR=HWPostil1 EVENT=NotifyCtrlReady>//初始化事件
	aip_init();
</SCRIPT>
