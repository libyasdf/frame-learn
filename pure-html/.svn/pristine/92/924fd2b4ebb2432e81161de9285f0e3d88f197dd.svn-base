.uploadifive-queue-item {
	/* background-color: lightYellow; */
	border-bottom:  1px dashed #c8c8c8;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	font: 14px '΢���ź�', Helvetica, Sans-serif;
	margin-top: 3px;
	/* padding: 15px; */
	padding: 7px;
}

.uploadifive-queue-item .close {
	background: url('uploadifive-cancel.png') 0 5px no-repeat;
	display: block;
	float: right;
	height: 16px;
	text-indent: -9999px;
	width: 16px;
}

.uploadifive-queue-item .progress {
	border: 5px solid #D0D0D0;
	height: 3px;
	margin-top: 5px;
	width: 100%;
}

.uploadifive-queue-item .progress-bar {
	background-color: #0072BC;
	height: 3px;
	width: 0;
}

input[type=file]{
	cursor: pointer;
}