header>.navbar .container-fluid {
	min-height: 88px;
	padding-right: 24px;
}

header>.navbar .navbar-brand {
	padding: 11px 0px 0px 52px;
}

header>.navbar .navbar-nav>li>a {
	position: relative;
	padding-top: 23px;
	padding-left: 8px;
	padding-right: 8px;
}

@media (min-width: 768px) {
	header>.navbar .navbar-nav>li:not(:last-child)>a:after {
		content: "";
		position: absolute;
		top: 24px;
		width: 1px;
		height: 18px;
		background: gray;
		right: 0px;
	}
}

header .nav>li>a>img {
	position: relative;
	top: -1px;
}

@media (max-width: 767px) {
	header>.navbar .container-fluid {
		height: auto;
	}
	header>.navbar .navbar-nav>li {
		text-align: center;
	}
	header>.navbar .navbar-brand {
		padding: 11px 0px 0px 0;
	}
}

@media (max-width: 496px) {
	header>.navbar .navbar-brand {
		width: 80%;
	}
	/*no_content样式begin*/
	.no_content img {
		width: 150px;
	}
	.no_content p {
		font-size: 14px;
	}
	/*no_content样式end*/
}

