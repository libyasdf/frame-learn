/* Tab 菜单标签页 */
.menuTab-Main {

}
.menu_tab_wrapper {
    overflow: hidden;
    position:relative;
    font-size: 0px;
/*    left: 235px;*/
    top: 120px;

}
.menu_tab_main {
    position: relative;
    height: 30px;
    line-height: 30px;
    margin-left:235px;
    margin-bottom:2px;
    border-top: 1px solid #dddddd;
    border-right: 1px solid #dddddd;
    border-bottom:2px solid #0c89cf;
    border-left: 1px solid #dddddd;
    background: #f5f5f5;
    white-space:nowrap;
    overflow: hidden;
}
.menu_tab_wrapper .tab_menu  {
    display: inline-block;
    position: absolute;
    vertical-align: top;
    padding: 0 30px;
    margin-bottom: 0;
    font-size: 0px;
    transition: left 0.5s;
    -moz-transition: left 0.5s;	/* Firefox 4 */
    -webkit-transition: left 0.5s;	/* Safari 和 Chrome */
    -o-transition: left 0.5s;	/* Opera */
}
.menu_tab_wrapper .tab_menu li {
    display: inline-block;
    padding:0px 20px;
    font-size: 12px;
    border-right: 1px solid #dddddd;
    box-sizing: border-box;
    cursor: pointer;
}
.menu_tab_wrapper .tab_menu_acitve {
    height: 28px;
    color: #fff;
    background: #2589c9;
    border-right: none;
}
.menu_tab_wrapper .tab_btn {
    display: inline-block;
    font-size: 12px;
    width: 30px;
    height: 100%;
    border: none;
    color: #6f848e;
    background: #ffffff;
    cursor: pointer;
}
.menu_tab_wrapper .btn_left {
    border-right: 1px solid #dddddd;
    position: absolute;
    left:0;
    z-index: 10;
}
.menu_tab_wrapper .btn_right {
    position: absolute;
    right:0;
    height: 90%;
    border-left: 1px solid #dddddd;
}