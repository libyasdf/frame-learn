.formpage_table{ font-size: 14px;  margin-bottom: 10px;}
.formpage_table td{ padding: 5px 2px; vertical-align: top;}
.formpage_table .title{ text-align: right; line-height: 26px; }
.formpage_table .form_textarea{border:1px solid #c5c5c5;background: #ffffff;}

.form_btn_big{ padding: 10px 25px;}

.table_add{ text-align: center; background: #f0f5ff; font-size: 12px;}
