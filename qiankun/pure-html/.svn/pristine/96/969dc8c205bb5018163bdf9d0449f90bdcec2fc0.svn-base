/*----3月5日 导航下拉样式修改------*/
.nav .open > a, .nav .open > a:hover, .nav .open > a:focus{ background: none; text-decoration: none;}
.nav .dropdown-menu{ background: #0073aa;}
.nav ul li .dropdown-menu li{ border: none; float: none; display: block; line-height: 40px;}
.nav ul li .dropdown-menu li a{ padding: 0; line-height: 40px;}
.nav ul li .dropdown-menu li a:hover{background: none;}

.nav .dropdown-menu > .active > a, .nav .dropdown-menu > .active > a:hover, .nav .dropdown-menu > .active > a:focus{background-color: #085274}
a:focus{text-decoration: none;}

/*----Tab标签(3月7日)-----*/
.block_title .nav{ background: none; min-width: auto;}
.block_title .nav .active{ background: none; }
.block_title .block_tab li{ height: 40px; line-height: 40px; float: left; font-size: 16px;}
.block_title .block_tab{ border: none; font-size: 16px;}
.block_title .block_tab li.active{  font-weight: bold; border-bottom: 3px solid #0073aa}
.block_title .block_tab li.active a{ color: #0073aa;}
.block_title .block_tab li a{ border: none; background: none;}
.block_title .block_tab li a:hover{ border: none; background: none;}
.block_tab > li.active > a, .block_tab > li.active > a:hover, .block_tab > li.active > a:focus{ border: none; background: none;}


