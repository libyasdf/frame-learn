                <form class="form-horizontal" id="form">
                <!-- resId -->
                <input name="resId" id="resId" type="hidden" value="">
                <input name="id" id="id" type="hidden" value="">
                <input name="superId" id="superId" type="hidden" value="">
                <input name="partyMemberId" id="partyMemberId" type="hidden" value="">
                <input name="partyOrganizationId" id="partyOrganizationId" type="hidden" value="">
                <input name="opertation" id="opertation" type="hidden" value="" />
              <!--  <input name="partyMember" id="partyMember" type="hidden" value=""-->
                <div class="container-fluid formpage_area" style="margin-bottom: 140px">
                    <!-- 基本信息 -->
                    <div class="row">
                        <div class="col-md-12 ">
                            <div class="block_title">
                                <span class="name">党员奖励</span>
                            </div>
                        </div>
                    </div>
                    <div class="row m-t-15">
                        <div class="col-md-12">
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"> 党员姓名：</label>
                                    <div class="col-md-4 form-view">
                                        <span id="partyMember" name="partyMember" ></span>
                                    </div>
                                </div>
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"> 党组织：</label>
                                    <div class="col-md-4 form-view">
                                        <span id="partyOrganization" name="partyOrganization" ></span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"> 奖励日期：</label>
                                    <div class="col-md-4 form-view">
                                        <span id="rewardTime" name="rewardTime" ></span>
                                    </div>
                                </div>
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"> 奖励机构：</label>
                                    <div class="col-md-4 form-view">
                                        <span id="rewardUnit" name="rewardUnit" ></span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"> 奖励内容：</label>
                                    <div class="col-md-4 form-view">
                                        <span id="rewardContent" name="rewardContent" ></span>
                                    </div>
                                </div>
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"> 奖励原因：</label>
                                    <div class="col-md-4 form-view">
                                        <span id="rewardReason" name="rewardReason" ></span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"> 批准机关级别：</label>
                                    <div class="col-md-4 form-view">
                                        <span id="approvalOrganLevel" name="approvalOrganLevel" ></span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"> 主要事迹：</label>
                                    <div class="col-md-10 form-view" style="word-break:break-all">
                                        <span id="mainDeeds" name="mainDeeds" ></span>
                                    </div>
                                </div>

                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"> 奖励撤销日期：</label>
                                    <div class="col-md-4 form-view">
                                        <span id="rewardRevokeTime" name="rewardRevokeTime" ></span>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="rowGroup">
                                    <label for="" class="col-md-2 control-label"> 备注：</label>
                                    <div class="col-md-10 form-view" style="word-break:break-all">
                                        <span id="remarks" name="remarks"></span>
                                    </div>
                                </div>
                            </div>

                    </div>
                    <!-- 基本信息结束 -->
                </div>
                </div>
                </form>

<div class="form_btn_area" style="text-align: center;">
    <button class="btn btn-primary form_btn_area_btn2"  onclick="MyLayer.closeOpen()"><span class="glyphicon glyphicon-remove-circle" aria-hidden="true"></span> 关闭</button>
</div>

