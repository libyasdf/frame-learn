<script src="/static/js/common/tablelist.js"></script>
<script type="text/javascript">
    laydate.render({
        elem: '#rewardTime' //指定元素
        ,done: function(value, date, endDate){
            $('#rewardTime').val(value).change()
        }
    });
    laydate.render({
        elem: '#rewardRevokeTime' //指定元素
        ,done: function(value, date, endDate){
            $('#rewardRevokeTime').val(value).change()
        }
    });


</script>
