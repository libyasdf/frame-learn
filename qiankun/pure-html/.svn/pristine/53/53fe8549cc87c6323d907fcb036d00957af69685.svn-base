<script type="text/javascript">
    laydate.render({//开始时间
        elem: '#beforeTime' //指定元素
        ,done: function(value, date, endDate){
            $('#beforeTime').val(value).change()
            var beginDate=$("#beforeTime").val();
            var endDate="";
            var nameVal ="";
            if($('#endTime').val()!=""){
                endDate =$('#endTime').val();
                if(isTimeSize(beginDate,endDate)){
                    $("#beforeTime").val("").change();
                    layer.msg("开始时间不能晚于截止时间!", {icon: 7});
                } ;
            }


        }
    });
    laydate.render({//截止时间
        elem: '#endTime' //指定元素
        ,done: function(value, date, endDate){
            $('#endTime').val(value).change()
            var beginDate="";
            var endDate="";
            if( $('#beforeTime').val()!=""){
                beginDate=$('#beforeTime').val();
                endDate =$('#endTime').val();
                if(isTimeSize(beginDate,endDate)){
                    $("#endTime").val("").change();
                    layer.msg("截止时间不能早于开始时间!", {icon: 7});
                } ;
            }

        }
    });



            $(function () {
        $('.nav-tabs > li').unbind('click').bind('click',function(){
