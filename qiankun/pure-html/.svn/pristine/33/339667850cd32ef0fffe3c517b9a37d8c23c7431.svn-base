	<img id="freenode1" style="display:none;" src="../image/flowstatus/freenode1.png"/>
	<img id="freenode2" style="display:none;" src="../image/flowstatus/freenode2.png"/>
	<img id="freenode3" style="display:none;" src="../image/flowstatus/freenode3.png"/>
	<img id="autonode1" style="display:none;" src="../image/flowstatus/autonode1.png"/>
	<img id="autonode2" style="display:none;" src="../image/flowstatus/autonode2.png"/>
	<img id="autonode3" style="display:none;" src="../image/flowstatus/autonode3.png"/>
	<img id="merage1" style="display:none;" src="../image/flowstatus/merage1.png"/>
	<img id="merage2" style="display:none;" src="../image/flowstatus/merage2.png"/>
	<img id="merage3" style="display:none;" src="../image/flowstatus/merage3.png"/>
	<img id="start1" style="display:none;" src="../image/flowstatus/start1.png"/>
	<img id="start2" style="display:none;" src="../image/flowstatus/start2.png"/>
	<img id="start3" style="display:none;" src="../image/flowstatus/start3.png"/>
	<img id="end1" style="display:none;" src="../image/flowstatus/end1.png"/>
