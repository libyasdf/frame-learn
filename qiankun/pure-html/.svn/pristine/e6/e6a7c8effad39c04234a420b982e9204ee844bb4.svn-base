@charset "utf-8";
/* CSS Document */
header{
	background:url(../image/bg_header.png) repeat-x;
	color:#fff;   
	height:50px;
	padding:0 15px;
	font-family:"微软雅黑", "微软雅黑 Light";
	}
.logo{
	float:left;
	margin-top:9px;
	margin-right:60px;
}
header,.content{
	min-width:1100px;
	}
	
header a{
	color:#fff;
	
}
header .nav > li > a {
	font-family:"微软雅黑", "微软雅黑 Light";
}
header .nav > li > a{
	padding-right:7px;
	padding-left:7px;
	font-size:16px;
	padding-bottom:13px;
	outline:none;
}
header .nav > li > a .small{
	margin-left:3px;
}
header .nav > li > a img{
	margin-top:-6px;
}
header .nav > li > a:hover,header .nav > li > a:focus{
	background:#205BB7;
	color:#fff;
	}
header .nav > li.line a{
	border-right:1px solid #6e91d0;
	padding:2px 0px;
	margin-top:13px;
	cursor:default;
	margin-left:7px;
	margin-right:7px;
}
header .nav > li.line a:hover{
	background:none;
}
header .nav > li.dropdown > a:hover,header .nav > li.dropdown > a:focus,header .open > .dropdown-toggle.btn-default, 
header .nav .open > a:hover,
header.nav .open > a:focus,
header .nav .open > a
{
	background:#205BB7;
	color:#fff;
}
header .dropdown-menu > li > a{
	outline:none;
}
header .nav > li.dropdown .glyphicon {
	font-size:12px;
	margin-left:3px;
}
header .nav .open > a, header .nav .open > a:hover,header .nav .open > a:focus{
	border:0;
}
header .navbar-right{
	margin-top:13px;
}
header  .navbar-right input{
	margin-right:5px;
	float:right;
}
header  .navbar-right span{
	display:block;
	float:right;
}
