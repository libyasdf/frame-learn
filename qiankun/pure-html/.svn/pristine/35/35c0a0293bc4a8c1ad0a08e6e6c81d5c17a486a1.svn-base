<!DOCTYPE html >
<html>
	<head>
		<meta name ="renderer"  content="webkit"/>
		<meta http-equiv="pragma" content="no-cache"/> 
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		<meta http-equiv="Cache-Control" content="no-store, must-revalidate"/>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
		<meta http-equiv="cache-control" content="no-cache"/>
		<link rel="stylesheet" type="text/css" href="../../../product/workflow/css/form/form_basic_NEW.css"/>
		<link rel="stylesheet" type="text/css" href="../../../plugins/uploadifive/assets/css/uploadifive/uploadifive.css"/>
		<style>
			div#IDEA_AREA{padding-left:25px; margin-left:-25px}
			.upload_button{
				height: 20px;
			    line-height: 20px;
			    margin-left: 35px;
			    overflow: hidden;
			    padding: 5px;
			    position: relative;
			    text-align: center;
			    width: 100px;
			}
		</style>
		
		<script type="text/javascript" src="../../../common/js/jquery-1.11.2.min.js"></script>
		<script type="text/javascript" src="../../common/js/lhgdialog/lhgdialog.min.js"></script>
		<!-- 页面获取参数 -->
		<script type="text/javascript" src="../../../common/js/getrequest.js"></script>
		<!-- 流程相关 -->
		<script type="text/javascript" src="../../../common/js/config.js"></script>
		<script type="text/javascript" src="../../common/js/commonFunction.js"></script>
		<script type="text/javascript" src="../../product/workflow/js/subtoflow/storage.js"></script>
		<!-- 提交表单验证 -->
		<script type="text/javascript" src="../../common/js/checkForm/autocheckform.js"></script>
		<script type="text/javascript" src="../../common/js/checkForm/checkform.js"></script>
		<!-- 获取cookie值 -->
		<script type="text/javascript" src="../../product/workflow/js/oa/getCook.js"></script>
		<!--选择时间插件  -->
		<script type="text/javascript" src="../../product/workflow/js/common/calendar/WdatePicker.js"></script>
		<!-- 自定义意见 -->
		<script type="text/javascript" src="../../../common/html/notion/notion.js"></script>
		<!-- 附件兼容 -->
		<script type="text/javascript">
		if(BrowserType()=='0'||BrowserType()=='IE8'||BrowserType()=='IE9'){
			$("body").append('\<script  src="../../../../product/workflow/js/jquery.uploadify.js">\<\/script\>'); 
			$("body").append('\<script src="../../../oa/js/fileupflash2.0.js">\<\/script\>'); 
			fileupNew("referenceFile", "referencequeue","referencedata", "referenceFileIds","pdf");
			fileupNew("attachmentFile", "attachmentqueue","attachmentdata", "attachmentFileIds");
		}else{
			$("body").append('\<script  src="../../../../plugins/uploadifive/assets/js/jquery.uploadifive.js">\<\/script\>'); 
			$("body").append('\<script src="../../../product/workflow/js/fileup2.0.js">\<\/script\>'); 
			fileup("referenceFile", "referencequeue","referencedata", "referenceFileIds","pdf");
			fileup("attachmentFile", "attachmentqueue","attachmentdata", "attachmentFileIds");
		}
		</script>
		<!-- 引入自己模块功能的js -->
		<script type="text/javascript" src="../../common/js/workFlowForm.js"></script>
		
		
	</head>
	<body  class="inForm" >
	<div id="TEMPLATE" class="newexlist select">
			<!-- 按钮区 -->
			<div id="BUTTON_AREA">
				<div id="flydiv"> 
					<div id="btn">
						<input id="preserveBtn" type="button" value="保&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;存" onclick="comitForm()" 
							class="preserveBtn"/>
						<div id="subflow" style="display: none;">
							<input name="bc" id="referBtn" type="button" value="发&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;送" onclick="" 
								class="referBtn"/>
						</div>
						<div id="flowCourse" style="display: none;">
							<input name="bc" id="courJi" type="button" value="流程记录"  
								class="Theprocessofpaper"/>
						</div>
						<div id="flowIdea" style="display: none;">
							<input name="bc" id="ideaYi" type="button" value="流程意见"  
								class="Theprocessofpaper"/>
						</div>
						<div id="flowstatus">
							<input name="bc" id="statIm" type="button" value="流  程  图" onclick="branchToCourse('2')"
								class="Theprocessdrawings"/>
						</div>
						<div id="remove" style="display: none;">
							<input name="removeBtn" id="removeBtn" type="button" value="撤&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;办" onclick="" 
								class="Theprocessofpaper"/>
						</div>
						<div id="approval" style="display: none;">
							<input name="approvalBtn" id="approvalBtn" type="button" value="审批通过" onclick="" 
								class="Theprocessofpaper"/>
						</div>
						<div id="noApproval" style="display: none;">
							<input name="noApprovalBtn" id="noApprovalBtn" type="button" value="审批不通过" onclick="" 
								class="Theprocessofpaper"/>
						</div>
						<input id="close" type="button" value="关&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;闭" onclick="window.close();"
							   class="closenew"/>
					</div>
			
				</div>
			</div>
			<!-- 表单start -->
			<form id="frm" method="post" action="" name="frm">
				<input name="id" id="id" type="hidden"/>
				<input name="filetypeid" id="filetypeid" type="hidden"/>
				<input name="workflowid" id="workflowid" type="hidden"/>
				<input name="workitemid" id="workitemid" type="hidden"/>
				<input name="sysId"id="sysId" type="hidden"/>
				<input name="orgId"id="orgId" type="hidden"/>
				<input name="attr" id="attr" type="hidden" value="3"/><!-- 代办表中的标识 ，需要在config.js中配置 -->
				<input name="attr1" id="attr1" type="hidden" value="leaveInfo"/><!--leaveInfo是模块标识，需要在config.js中配置   -->
				<input name="opertation" id="opertation" type="hidden"/>
			
			<!-- 功能模块表单项 start-->
			
			        <!-- 表单基本信息 start-->
			        
			        <!-- 表单基本信息 end-->
		            <!-- 附件相关 start-->
		            <div id="showcontentUpload" style="display: none;">
						<div class="inForm legend">
							正文文件
							<div style="float: right;" class="leftShift">
								<a href="javascript:void(0)" class="specialEffects"
									onclick="CIEbank.operaDiv('showTSsxopera')">展开/收起</a>
							</div>
						</div>
						<fieldset id="showTSsxopera">
							<div>
								<div id="some_file_queue"></div>
								<div id="showreferencequeue"></div>
								<div>
									<input type="hidden" name="showreferencedata" id="showreferencedata">
									<input type="hidden" name="showreferencedeldata" id="showreferencedeldata"> 
									<input type="hidden" name="showreferenceFileIds" id="showreferenceFileIds">
								</div>
							</div>
						</fieldset>
					</div>
					<div id="attmentUpload" style="display: none;">
						<div class="inForm legend">
							附件文件
							<div style="float: right;" class="leftShift">
								<a href="javascript:void(0)" class="specialEffects"
									onclick="CIEbank.operaDiv('FWSHopera')">展开/收起</a>
							</div>
						</div>
						<fieldset id="FWSHopera">
							<div>
								<div id="some_file_queue"></div>
								<div style="margin-top: 3px; padding-left: 50px;">
									附件：<input type="file" id="attachmentFile" name="attachmentFile"
										class="" />
								</div>
								<div id="attachmentqueue"></div>
								<div>
									<input type="hidden" name="attachmentdata" id="attachmentdata">
									<input type="hidden" name="attachmentdeldata"id="attachmentdeldata"> 
									<input type="hidden" name="attachmentFileIds" id="attachmentFileIds">
								</div>
							</div>
							<!-- <script type="text/javascript">
								fileup("attachmentFile", "attachmentqueue","attachmentdata", "attachmentFileIds");
							</script> -->
						</fieldset>
					</div>
					<div id="showattmentUpload" style="display: none;">
						<div class="inForm legend">
							附件文件
							<div style="float: right;" class="leftShift">
								<a href="javascript:void(0)" class="specialEffects"
									onclick="CIEbank.operaDiv('showFWSHopera')">展开/收起</a>
							</div>
						</div>
						<fieldset id="showFWSHopera">
							<div>
								<div id="some_file_queue"></div>
								<div id="showattachmentqueue"></div>
								<div>
									<input type="hidden" name="showattachmentdata" id="showattachmentdata">
									<input type="hidden" name="showattachmentdeldata" id="showattachmentdeldata"> 
									<input type="hidden" name="showattachmentFileIds" id="showattachmentFileIds">
								</div>
							</div>
						</fieldset>
					</div>
		            <!-- 附件相关 end-->
		            
		            <!-- 意见域显示 start-->
				    <div id="ideaarea" class="inForm legend idea" style="display: none">意见域
					<div style="float: right;"  class="leftShift">
						<a href="javascript:void(0)" class="specialEffects" onclick="CIEbank.operaDiv('tridea')">展开/收起</a>
					</div>
					</div>	
					<fieldset id="tridea" class="idea" style="padding: 5px 0px 5px 0px;display: none">
						 <div><table><tbody>
						 	<tr style="border-bottom:1px dashed #c8c8c8"><td>
						 		<textarea  id="idea" name="idea" attrType="textarea" CK_TYPE="MaxLen_2000" CK_NAME="意见" style="width:100%;height:80px;border:0px;" ></textarea>
						 	</td></tr>
						 	<tr id="shownotiontrutil"><td align="center">
								<table style="width:100%" cellspacing="0" cellpadding="0" border="0"><tbody>
									<tr style="border-bottom:1px dashed #c8c8c8">
										<td style="text-align:center;" width="70">
											<font>常用语：</font>
										</td>
										<td align="left">
											<select name="CuOpinion" onchange="onSelect(this.value)" id="CuOpinion" style="width:300px;border:1px solid #ccc">
												<option value="">--请选择--</option>
											</select>
											<input class="dow" style="width: 100px;height: 25px;" onclick="add()" value="添加常用意见" type="button">	
										</td>
									</tr>
								</tbody></table>
							</td></tr>
						</tbody></table></div>
					</fieldset>
				<!-- 意见域显示 end-->
				
			<!-- 功能模块表单项 end-->
			</form>
			<!-- 表单 end-->
		</div>
