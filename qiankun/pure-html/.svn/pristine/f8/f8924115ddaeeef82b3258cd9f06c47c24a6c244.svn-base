<script type="text/javascript">

    $('.nav-tabs > li').unbind('click').bind('click', function () {
        var id = $(this).attr('for');
        $(this).addClass('active').siblings('li').removeClass('active');
        $('.main').children().addClass('hidden');
        $('#' + id).removeClass('hidden');
        var index = id.substr(id.length-1,1);
        tableInit('right_table'+index);
        $("#featsType").val(index);

    })


    function preview(){
        var resId = $('#resId').val();
        var year = $('#year').val();
        window.open('/modules/mypage/honorWall/honorWall.html?resId=' + resId +'&year=' + year);
    }


</script>

