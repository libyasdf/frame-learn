<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta http-equiv="Expires" content="0">
    <meta http-equiv="Pragma" content="no-cache">
    <meta http-equiv="Cache-control" content="no-cache">
    <meta http-equiv="Cache" content="no-cache">

    <title>表单页面</title>
    <link rel="stylesheet" href="../../static/core/bootstrap/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="../../static/core/bootstrapvalidator/dist/css/bootstrapValidator.min.css"/>
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="../../static/core/html5/html5shiv.min.js"></script>
    <script src="../../static/core/html5/respond.js"></script>
    <![endif]-->
    <link href="../../static/css/common/style.css" rel="stylesheet"/>
    <link href="../../static/css/common/form-public.css" rel="stylesheet"/>

</head>

<body class="formpage_bj">
<div class="container-fluid formpage_area">
    <form class="form-horizontal" id="form">
        <!-- 基本信息 -->
        <div class="row">
            <div class="col-md-12 ">
                <div class="block_title">
                    <span class="name">基本信息</span>
                </div>
            </div>
        </div>
        <div class="row m-t-15">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="" class="col-md-2 control-label">信访人姓名：</label>
                    <div class="col-md-4 form-view">
                        <span name="name"></span>
                    </div>
                    <label for="" class="col-md-2 control-label">性别：</label>
                    <div class="col-md-4 form-view">
                        <span name="sex"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="col-md-2 control-label">民族：</label>
                    <div class="col-md-4 form-view">
                        <span name="nation"></span>
                    </div>
                    <label for="" class="col-md-2 control-label">出生日期：</label>
                    <div class="col-md-4 form-view">
                        <span name="birth"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="col-md-2 control-label">是否保密：</label>
                    <div class="col-md-4 form-view">
                        <span name="isPrivacy"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="col-md-2 control-label">备注：</label>
                    <div class="col-md-10  form-view">
                        <span name="remark"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="col-md-2 control-label">我方人数：</label>
                    <div class="col-md-4 form-view">
                        <span name="ourPeopleNum"></span>
                    </div>
                    <label for="" class="col-md-2 control-label">对方人数：</label>
                    <div class="col-md-4 form-view">
                        <span name="peerPeopleNum"></span>
                    </div>
                </div>
            </div>
        </div>
        <!-- 基本信息结束 -->

        <!-- 其他人员信息 -->
        <div class="row">
            <div class="col-md-12 ">
                <div class="block_title">
                    <span class="name">xxx方其他人员</span>
                </div>
            </div>
        </div>
        <div class="row m-t-15" style="margin-top: 15px;">
            <div class="col-md-12 p-f-60">
                <!--这里表格用的普通表格，要用bootstrap-table（参考列表页面）可以自行修改-->
                <div style="margin-bottom: 5px;">
                    <!--<button class="list_table_btn2 allSelect" type="button">
                        <span class="glyphicon glyphicon-plus-sign"></span> 全选
                    </button>-->
                    <button class="list_table_btn2 addTR" type="button">
                         <span class="glyphicon glyphicon-plus-sign"></span> 添加
                    </button>
                    <button class="list_table_btn2 allDelete" type="button">
                        <span class="glyphicon glyphicon-remove"></span> 删除
                    </button>
                </div>
                <table class="table table-bordered" name="table" id="table">
                    <thead>
                    <tr>
                        <th width="5%" class="text-center"><input type="checkbox" class="allSelect"></th>
                        <th width="5%" class="text-center">序号</th>
                        <th width="12%" class="text-center">姓名</th>
                        <th width="18%" class="text-center">身份证号</th>
                        <th width="18%" class="text-center">地址</th>
                        <th class="text-center">备注</th>
                        <th width="10%" class="text-center">操作</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr>
                        <td class="text-center">1</td>
                        <td class="text-center">
                            <input type="text" class="form-control" name="name">
                        </td>
                        <td class="text-center">
                            <input type="text" class="form-control"  name="IDCar">
                        </td>
                        <td class="text-center">
                            <input type="text" class="form-control" name="addr">
                        </td>
                        <td>
                            <input type="text" class="form-control" name="remk">
                        </td>
                        <td class="text-center">
                            <span class="glyphicon glyphicon-ok"></span>
                            <span class="glyphicon glyphicon-edit hidden"></span>
                            <span class="glyphicon glyphicon-remove"></span>
                        </td>
                    </tr>
                    </tbody>
                </table>

            </div>
        </div>

        <!-- 我方人员信息 -->
        <div class="row">
            <div class="col-md-12 ">
                <div class="block_title">
                    <span class="name">我方其他人员</span>
                </div>
            </div>
        </div>
        <div class="row m-t-15" style="margin-top: 15px;">
            <div class="col-md-12 p-f-60">
                <div style="margin-bottom: 5px;">
                    <!--<button class="list_table_btn2 allSelect" type="button">
                        <span class="glyphicon glyphicon-plus-sign"></span> 全选
                    </button>-->
                    <button class="list_table_btn2 addTR" type="button">
                        <span class="glyphicon glyphicon-plus-sign"></span> 添加
                    </button>
                    <button class="list_table_btn2 allDelete" type="button">
                        <span class="glyphicon glyphicon-remove"></span> 删除
                    </button>
                </div>
                <table class="table table-bordered" name="table" id="table2" style="margin-bottom: 5px;">
                    <thead>
                    <tr>
                        <th width="5%" class="text-center"><input type="checkbox" class="allSelect"></th>
                        <th width="5%" class="text-center">序号</th>
                        <th width="12%" class="text-center">姓名</th>
                        <th width="18%" class="text-center">身份证号</th>
                        <th width="18%" class="text-center">地址</th>
                        <th class="text-center">备注</th>
                        <th width="10%" class="text-center">操作</th>
                    </tr>
                    </thead>
                    <tbody>
                   
                    </tbody>
                </table>
                <ul id="pageLimit" style="margin-top: 0;float: right;"></ul>
            </div>
        </div>
        <!-- 我方人员信息结束 -->
    </form>
</div>

<!-- 按钮区 -->
<div class="form_btn_area" style="text-align: center;">
    <button class="btn btn-danger" onclick="MyLayer.closeOpen()">
        关&nbsp;&nbsp;闭
    </button>

</div>
<!-- 按钮区结束 -->

<script src="../../static/core/jquery/jquery-1.11.2.min.js"></script>
<script src="../../static/core/bootstrap/js/bootstrap.min.js"></script>
<script src="../../static/core/layer/layer.js"></script>
<script type="text/javascript" src="../../static/core/bootstrap-paginator/src/bootstrap-paginator.js"></script>
<script src="../../static/js/common/mylayer.js"></script>
<script src="../../static/js/common/pageEcho.js"></script>
<script type="text/javascript" src="../../static/js/common/DynamicTable.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        // 加载数据
        $.getJSON("../../mock/readOnly.json", function (data) {
            PageEcho.parseData({
                data: data,
                tableFun: function (key, json) {
                    var data = json.data;
                    var html = [];
                    for (var i = 0; i < data.length; i++) {
                        html.push('<tr>');
                        html.push('<td class="text-center" >' + data[i].id + '</td>');
                        html.push('<td class="text-center" >' + data[i].name + '</td>');
                        html.push('<td class="text-center" >' + data[i].age + '</td>');
                        html.push('<td class="text-center" >' + data[i].addr + '</td>');
                        html.push('<td ><input type="email" class="form-control" id="exampleInputEmail1" placeholder="输入备注"></td>');
                        html.push('</tr>');
                    }
                    // $('[name=' + key + ']').find('tbody').empty().append(html.join(''));
                }
            });
        });
    })


    new DynamicTable({
        "tableId":"table",
        "getUrl":"../../mock/DynamicTable.json"
    }).init();

    new PagingTable({
        "tableId":"table2",
        "pageId":"pageLimit",
        "getUrl":"../../mock/DynamicTable.json"
    }).init();
</script>
</body>
</html>
 