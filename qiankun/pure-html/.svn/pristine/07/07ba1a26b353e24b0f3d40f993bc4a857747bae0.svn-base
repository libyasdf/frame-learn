
/**页面样式重组*/

/*部分页面样式重置**/
* { padding:0; margin:0;} 
input{background:none;}
th,td{margin:0;padding:0;overflow:hidden}
table{border-collapse:collapse;border-spacing:0;}
fieldset,img,legend,input{border:0;margin: 0;padding: 0;}



/**===========================页面公用样式=====================*/
body{background-color:#f5f5f5;font-family: "微软雅黑","Microsoft YaHei";height:100%;font-size:14px;}
input,textarea,select{font-family: "微软雅黑","Microsoft YaHei";font-size:14px;}
select{width: auto;border:none;border:1px solid #c8c8c8;}
div.exlist table{width:100%;height:100%;}
div.newexlist table{width:100%;height:100%;}
table tr td span.need{color: red}
li{display:block}
a,img{cursor: pointer;}
a{text-decoration: none}
input,textarea{word-break: break-all;word-wrap: break-word;}
div{word-wrap:break-word; word-break:break-all;}
#fb div{word-wrap:break-word; word-break:break-all;} 

/**table中两列*/
.Stl{width:16%;text-align:right;}/*table中标题*/
.Str{width:34%;}/*table中内容*/

/**table中三列*/
.ttl{width:12%;text-align:right;}/*table中标题*/
.ttl.long{width:14%;text-align:right;}
.ttr{width:21%;}/*table中内容*/
.ttr.long{width:19%;}


/*=================================表单公用样式=============================================*/
.addTopClass{margin-top:20px}
.exlist{margin:30px auto;padding:40px;width:72%;height:auto;background:url("../../image/formBackground.png") repeat scroll !important;position:relative;top:0px;}
div#mainBody .exlist{margin:30px auto;padding-top:40px;padding-left:0px;padding-right:0px;padding-bottom:0px;width:72%;height:auto;background:url("../../image/formBackground.png") repeat scroll !important;}
.newexlist{background:0;padding:0px;margin:0 auto;width:72%;height:auto;}
.pageStyle{background:url("../../image/formBackground.png") repeat scroll !important;width:100%;padding:40px;padding-top:50px;padding-right:18px;padding-left:18px;margin-bottom: 40px;border: 3px solid #e8e8e8;border-radius: 5px;}
div#mainBody .pageStyle{background:url("../../image/formBackground.png") repeat scroll !important;width:100%;padding-top:40px;padding-left:0px;padding-right:0px;padding-bottom:0px;margin-bottom: 40px;border: 3px solid #e8e8e8;border-radius: 5px;}
div.exlist.delayexit,div.exlist.feedabckexit{width:80%;}
div.newexlist.delayexit,div.newexlist.feedabckexit{width:80%;}
.legend{height:auto;font-weight:bolder;font-size:20px;line-height:35px;padding-bottom:20px;width:auto;}
#title{border-bottom: 2px solid #df2121;}
#title center {font-size: 2.5em;font-family: 黑体;color:#df2121;}
#title center ul#JBTitle{font-size: 26px;display: inline-flex;}
#title center ul#JBTitle.unchecked{border: 2px solid #DEDEDE;}
#title center ul#JBTitle.checked{border: 2px solid #df2121;}

#divtitle{border-bottom: 2px solid #df2121;}
#divtitle center {font-size: 2.5em;font-family: 黑体;color:#df2121;}
#divtitle center ul#JBTitle{font-size: 26px;display: inline-flex;}
#divtitle center ul#JBTitle.unchecked{border: 2px solid #DEDEDE;}
#divtitle center ul#JBTitle.checked{border: 2px solid #df2121;}

ul#JBTitle li{padding:3px;border: none;cursor:pointer;}
ul#JBTitle li.nocheckli{background: #f5f5f5;color: #dedede;float:left;line-height:26px;}
ul#JBTitle li.checkli{background: none;color: #df2121;float:left;line-height:26px;}
div.legend {height: 30px;color: #df2121;font-weight: bolder;font-size: 1.4em;line-height: 30px;padding: 0 10px;background: url("../../image/titlbook.png") no-repeat scroll left !important;padding-left: 25px;}
div.inFormTitle{height: 30px;color: #df2121;font-weight: bolder;font-size: 1.4em;line-height: 30px;padding: 0 10px;margin-bottom:30px;padding-left: 25px;padding-top: 20px;}
div.inFormTitle input{background: #ffffff;border: 1px solid #dedede;width: 87%;height: 18px}
fieldset {width: 92%;border: 1px solid #dedede;border-top: 3px solid #dedede;margin: 10px auto;background: #ffffff;margin-bottom: 30px;padding:5px 0px 15px 0px ;}
select{width:auto;border-color: #c8c8c8;}

/*=================================表单当前状态标识样式=================================*/
#mainBody .stateBtnClass{position:absolute;top:113px;width:400px;}
.stateBtnClass{position:absolute;top:-3px;}
.stateBtnClass img{margin:0px !important;}
.hintDiv{margin-top:10px;margin-bottom:10px;diaplay:none}
.hintLabel{margin-left:4%;color:red;display:block;}
