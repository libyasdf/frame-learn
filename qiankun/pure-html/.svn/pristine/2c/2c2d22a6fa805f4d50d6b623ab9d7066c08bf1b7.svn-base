.td-nav-list {
	margin: 30px 0px 0px; padding: 0px; background-color: rgb(255, 255, 255);
}
.td-nav-list > li > a {
	margin: 0px 0px -1px; padding: 8px 14px !important; border: 1px solid rgb(229, 229, 229); display: block;
}
.td-nav-list > .active {
	border-left-color: rgb(221, 75, 57); border-left-width: 3px; border-left-style: solid;
}
.td-nav-list > .active > a {
	padding: 8px 14px 8px 12px !important; border-left-color: currentColor !important; border-left-width: 0px !important; border-left-style: none !important; position: relative; z-index: 2; text-shadow: 0 1px 0 rgba(0, 0, 0, .15);
}
.td-nav-list > li > a:hover {
	background-color: rgb(245, 245, 245);
}
.td-nav {
	background: rgb(247, 247, 247); padding: 0px 10px; height: 40px; line-height: 40px; border-bottom-color: rgb(229, 229, 229); border-bottom-width: 1px; border-bottom-style: solid; box-shadow: 0px 1px 4px rgba(0,0,0,0.065); -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.065); -moz-box-shadow: 0 1px 4px rgba(0, 0, 0, 0.065);
}
.td-nav-table {
