.select2-container .select2-choice {
	border-width: 1px; border-style: solid; border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgb(162, 162, 162); border-radius: 4px; height: 28px; color: rgb(51, 51, 51); line-height: 28px; filter: progid:DXImageTransform.Microsoft.gradient(enabled=false); box-shadow: inset 0px 1px 0px rgba(255,255,255,0.2), 0px 1px 2px rgba(0,0,0,0.05); background-repeat: repeat-x; background-color: rgb(245, 245, 245); text-shadow: 0 1px 1px rgba(255, 255, 255, 0.75); -webkit-border-radius: 4px; -moz-border-radius: 4px; -webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2), 0 1px 2px rgba(0, 0, 0, 0.05); -moz-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2), 0 1px 2px rgba(0, 0, 0, 0.05);
}
.select2-container .select2-choice:hover {
	background-position: 0px -15px; text-decoration: none; background-color: rgb(230, 230, 230); -webkit-transition: background-position 0.1s linear; -moz-transition: background-position 0.1s linear; -o-transition: background-position 0.1s linear; transition: background-position 0.1s linear;
}
.select2-container .select2-choice div {
	text-align: center; border-top-right-radius: 4px; border-bottom-right-radius: 4px; -webkit-border-top-right-radius: 4px; -moz-border-radius-topright: 4px; -webkit-border-bottom-right-radius: 4px; -moz-border-radius-bottomright: 4px;
}
.select2-container .select2-choice div b {
	width: 0px; height: 0px; margin-top: 12px; vertical-align: top; border-top-color: rgb(0, 0, 0); border-right-color: transparent; border-left-color: transparent; border-top-width: 4px; border-right-width: 4px; border-left-width: 4px; border-top-style: solid; border-right-style: solid; border-left-style: solid; display: inline-block; content: "";
}
.select2-container-active.select2-dropdown-open .select2-choice {
	border-bottom-right-radius: 0px; border-bottom-left-radius: 0px; -webkit-border-bottom-left-radius: 0; -moz-border-radius-bottomleft: 0; -webkit-border-bottom-right-radius: 0; -moz-border-radius-bottomright: 0;
}
.select2-drop {
	border-width: 0px 1px 1px; border-style: solid; border-color: transparent rgb(230, 230, 230) rgb(191, 191, 191);
}
#loading_layer {
	background: rgb(255, 255, 255); left: 0px; top: 0px; width: 100%; height: 100%; display: block !important; visibility: visible; position: absolute; z-index: 10000;
}
#loading_layer img {
	margin: -5px 0px 0px -32px; left: 50%; top: 50%; width: 64px; height: 10px; position: fixed;
}
#scrollTop {
	background: url("../images/common/scroll-top.png") no-repeat center rgb(193, 193, 193); border-radius: 6px; width: 50px; height: 50px; right: 60px; bottom: 20px; margin-bottom: 5px; display: none; position: fixed; z-index: 2147483647; cursor: pointer; -webkit-transition: background 1s; -moz-transition: background 1s; -o-transition: background 1s;
}
.attachment-wrapper {
	height: 25px; line-height: 25px; margin-right: 5px; display: inline-block;
}
.attachment-wrapper .dropdown:hover ul.dropdown-menu {
	margin: 0px; padding: 2px 0px; display: block;
}
.attachment-wrapper a.dropdown-toggle {
	padding: 2px 1px;
}
.attachment-wrapper a.dropdown-toggle img {
	width: 20px; height: 20px;
}
.attachment-wrapper .dropdown {
	display: inline-block;
}
.attachment-wrapper span.size {
	font-size: 10px; font-style: italic;
}
.attachment-wrapper a.dropdown-toggle:hover {
	background: rgb(0, 102, 164); border-radius: 2px; color: rgb(255, 255, 255); text-decoration: none; -webkit-border-radius: 2px; -moz-border-radius: 2px;
}
.attachment-wrapper a.dropdown-toggle::after {
	content: none;
}
