<script type="text/javascript">
    laydate.render({//开始时间
        elem: '#startTime' //指定元素
        ,done: function(value, date, endDate){
            $('#startTime').val(value).change()
            var beginDate=$("#startTime").val();
            var endDate="";
            var nameVal ="";
            if($('#endTime').val()!=""){
                endDate =$('#endTime').val();
                if(isTimeSize(beginDate,endDate)){
                    $("#startTime").val("").change();
                    layer.msg("开始时间不能晚于结束时间!", {icon: 7});
                } ;
            }


        }
    });
    laydate.render({//结束时间
        elem: '#endTime' //指定元素
        ,done: function(value, date, endDate){
            $('#endTime').val(value).change()
            var beginDate="";
            var endDate="";
            if( $('#startTime').val()!=""){
                beginDate=$('#startTime').val();
                endDate =$('#endTime').val();
                if(isTimeSize(beginDate,endDate)){
                    $("#endTime").val("").change();
                    layer.msg("结束时间不能早于开始时间!", {icon: 7});
                } ;
            }

        }
    });



    $(function () {
        $('.nav-tabs > li').unbind('click').bind('click',function(){
            var id = $(this).attr('for');
            $(this).addClass('active').siblings('li').removeClass('active');
            $('.main').children().addClass('hidden');
            $('#'+id).removeClass('hidden');
        });
    });

</script>

