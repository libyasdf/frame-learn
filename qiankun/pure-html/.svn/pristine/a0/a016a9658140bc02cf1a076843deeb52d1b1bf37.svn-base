<body class="formpage_bj">
<form class="form-horizontal" id="form">
    <!-- resId -->
    <input name="resId" id="resId" type="hidden" value="">
    <input name="id" id="id" type="hidden" value="">
    <input name="partyOrganizationId" id="partyOrganizationId" type="hidden" value="">
    <div class="container-fluid formpage_area" style="margin-bottom: 140px">
        <!-- 基本信息 -->
        <div class="row">
            <div class="col-md-12 ">
                <div class="block_title">
                    <span class="name">基本信息</span>
                </div>
            </div>
        </div>
        <div class="row m-t-15">
            <div class="col-md-12">
                <div class="form-group">
                    <div class="rowGroup">
                        <label for="" class="col-md-2 control-label"><b style="color: red;">*</b> 党组织：</label>
                        <div class="col-md-4">
                            <input type="text" class="form-control" value="" readonly id="partyOrganizationName" name="partyOrganizationName" >
                        </div>
                    </div>
                    <div class="rowGroup">
                        <label for="" class="col-md-2 control-label"> 主持人 ：</label>
                        <div class="col-md-4">
                            <input type="text" class="form-control" id="host" name="host">
                        </div>
                    </div>

                </div>
                <div class="form-group">
                    <div class="rowGroup">
                        <label for="" class="col-md-2 control-label"><b style="color: red;">*</b> 开始时间：</label>
                        <div class="col-md-4">
                            <input type="text" class="form-control" id="startTime" name="startTime">
                        </div>
                    </div>
                    <div class="rowGroup">
                        <label for="" class="col-md-2 control-label"><b style="color: red;">*</b> 结束时间：</label>
                        <div class="col-md-4">
                            <input type="text" class="form-control" id="endTime" name="endTime">
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="rowGroup">
                        <label for="" class="col-md-2 control-label"> 地点：</label>
                        <div class="col-md-10">
                            <input type="text" class="form-control" id="placeval" name="placeval">
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="rowGroup">
                        <label for="" class="col-md-2 control-label"> 主题：</label>
                        <div class="col-md-10">
                            <input type="text" class="form-control"  id="themeval" name="themeval">
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <div class="rowGroup">
                        <label for="" class="col-md-2 control-label"> 应到人数：</label>
                        <div class="col-md-4">
                            <input type="text" class="form-control"  id="peopleNumber" name="peopleNumber">
                        </div>
                    </div>
                    <div class="rowGroup">
                        <label for="" class="col-md-2 control-label"> 实到人数：</label>
                        <div class="col-md-4">
                            <input type="text" class="form-control" id="actualNumber" name="actualNumber">
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="rowGroup">
                        <label for="" class="col-md-2 control-label"> 主要内容：</label>
                        <div class="col-md-10">
                            <textarea class="form-control" rows="3" id="primaryCoverage" name="primaryCoverage"></textarea>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="rowGroup">
                        <label for="" class="col-md-2 control-label"> 整改措施落实情况：</label>
                        <div class="col-md-10">
                            <textarea class="form-control" rows="3" id="situation" name="situation"></textarea>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="col-md-2 control-label" > 出勤人员：</label>
                    <div class="col-md-10">
                        <textarea class="form-control" rows="3"  id="attendance" name="attendance" readonly></textarea>
                    </div>
                </div>
            </div>
        </div>
        <!-- 基本信息结束 -->
    </div>
</form>


<div class="form_btn_area" style="text-align: center;">
    <button id="save" class="btn btn-primary form_btn_area_btn2" onclick="commitForm();" ><span class="glyphicon glyphicon-floppy-disk" aria-hidden="true"></span> 保存</button>
</div>
