@charset "utf-8";

/* CSS Document */

* {
	padding: 0px;
	margin: 0px;
}

body {
	font-size: 12px;
	line-height: 25px;
	text-align: left;
	font-family: "微软雅黑", yahei;
	color: #404040;
	background: #ffffff;
}

a {
	text-decoration: none;
	color: #404040;
}

a:hover {
	color: #000000;
	text-decoration: underline;
}

img {
	border: 0px;
	vertical-align: middle;
}

li {
	list-style-type: none;
}

.clr {
	clear: both;
}

.fr {
	float: right;
}

.fl {
	float: left;
}

.red,
.red a {
	color: #ff0000;
}

.blue,
.blue a {
	color: #23739e;
}

.white,
.white a {
	color: #FFF;
}

.mt_20 {
	margin-top: 20px;
}

.m_5 {
	margin: 5px;
}

.m_10 {
	margin: 10px;
}

.mt_10 {
	margin-top: 10px;
}

.mr_10 {
	margin-right: 10px;
}

.mb_10 {
	margin-bottom: 10px;
}

.ml_10 {
	margin-left: 10px;
}

.mt_5 {
	margin-top: 5px;
}

.mr_5 {
	margin-right: 5px;
}

.mb_5 {
	margin-bottom: 5px;
}

.ml_5 {
	margin-left: 5px;
}

.p_5 {
	padding: 5px;
}

.p_10 {
	padding: 10px;
}

.pt_10 {
	padding-top: 10px;
}

.pr_10 {
	padding-right: 10px;
}

.pb_10 {
	padding-bottom: 10px;
}

.pl_10 {
	padding-left: 10px;
}

.pt_5 {
	padding-top: 5px;
}

.pr_5 {
	padding-right: 5px;
}

.pb_5 {
	padding-bottom: 5px;
}

.pl_5 {
	padding-left: 5px;
}

.center {
	margin-left: auto;
	margin-right: auto;
}

.tl {
	text-align: left;
}

.more {
	float: right;
	font-size: 12px;
	width: 50px;
}


/*-----------------头部样式----------------*/

.head {
	background: url(../images/head_bj.png) center #c7e9fa no-repeat;
	height: 75px;
	color: #000;
	min-width: 1002px;
	position: relative;
}

.head .logo {
	position: absolute;
	left: 15px;
	top: 5px;
}

.head .user {
	position: absolute;
	right: 10px;
	top: 23px;
	color: #085274;
}

.head .user a {
	color: #085274;
}

.index_nav {
	background: #0073aa;
	height: 40px;
	line-height: 40px;
	color: #FFF;
	position: relative;
	min-width: 1002px;
}

.index_nav ul {
	margin: 0;
}

.index_nav ul li {
	float: left;
	display: inline-block;
	height: 40px;
	padding-left: 15px;
	padding-right: 15px;
	font-size: 16px;
	border-right: 1px solid #006493
}

.index_nav ul li:hover {
	background: #085274;
}

.index_nav ul li a {
	color: #FFF;
}

.index_nav ul li a:hover {
	text-decoration: none;
	color: #FF9;
}

.index_nav .active {
	background: #085274;
	color: #FF9;
}


/*-----------------左侧导航样式----------------*/

.left {
	background: #ffffff;
}

.left-title {
	background: url(../images/zyqy_left02.jpg) repeat-x;
	text-align: center;
}

.left_ul a {
	color: #000000;
}

.left_ul a:hover {
	text-decoration: none;
	background: #2589c9;
	color: #FFF;
}

.left_ul .active {
	background: #2589c9;
	color: #FFF;
}

.left_ul .active a {
	color: #FFF;
}

.left-yiji {
	display: block;
	line-height: 24px;
	padding-top: 10px;
	padding-bottom: 10px;
	padding-left: 10px;
	padding-right: 10px;
	color: #000;
	background: url(../images/left.png) repeat-x #e0e0e0;
	overflow: hidden;
	white-space: nowrap;
	font-size: 14px;
}

.left-yiji .fr {}

.left-erji {
	display: block;
	padding-left: 20px;
	padding-right: 10px;
	line-height: 15px;
	padding-top: 10px;
	padding-bottom: 10px;
	overflow: hidden;
	white-space: nowrap;
	font-size: 13px;
}

.left-sanji {
	display: block;
	padding-left: 40px;
	padding-right: 10px;
	line-height: 15px;
	padding-top: 10px;
	padding-bottom: 10px;
	overflow: hidden;
	white-space: nowrap;
	font-size: 13px;
}


/*-----------------列表页面样式----------------*/

.zsw_right_title {
	background: #f9f9f9;
	border: #e7e7e7 1px solid;
	border-left: 4px solid #238cd0;
	height: 46px;
	line-height: 46px;
	padding-left: 10px;
	font-size: 16px;
}

.block_title {
	background: url(../images/titleline_05.jpg) repeat-x bottom;
	height: 40px;
	line-height: 40px;
	position: relative;
}

.block_title .name {
	background: url(../images/titleline_04.jpg) repeat-x bottom;
	display: inline-block;
	padding-left: 20px;
	padding-right: 20px;
	font-size: 16px;
	font-weight: bold;
	color: #0073aa;
}

.block_title .title_down {
	position: absolute;
	right: 15px;
	top: 5px;
}

.list_form_table .title {
	text-align: right;
}

.list_form_table td {
	padding: 5px 0px;
	line-height: 14px;
}

.form_btn {
	padding: 7px 10px;
	color: #ffffff;
	background: #238cd0;
	line-height: 14px;
	border: none;
	display: inline-block;
}

a.form_btn {
	color: #ffffff;
}

.form_btn:hover {
	background: #085274;
	text-decoration: none;
}

.form_input {
	border: 1px solid #cccccc;
	height: 27px;
	line-height: 25px;
	background: #ffffff;
}

.list_button_area {
	padding-left: 0px;
	padding-top: 5px;
	padding-bottom: 5px;
	padding-left: 10px;
	border: 1px solid #dddddd;
	border-bottom: none;
}

.list_table {
	border-bottom: 1px solid #dddddd;
	border-right: 1px solid #dddddd;
	text-align: center;
}

.list_table td {
	border-top: 1px solid #dddddd;
	border-left: 1px solid #dddddd;
	padding-top: 3px;
	padding-bottom: 3px;
}

.list_table .list-title {
	background: #f4f4f4;
	font-weight: bold;
}

.list_table .list-shen {
	background: #f4f4f4;
}

.list-huaguo {
	background: #dbe7ed;
}

a.list_table_btn {
	color: #c45801;
	text-decoration: underline;
	padding-right: 5px;
}

.list_table_btn2 {
	border: 1px solid #e7e7e7;
	border-radius: 3px;
	display: inline-block;
	padding: 6px 10px;
	line-height: 16px;
	background: url(../images/btn_bj.png) repeat-x #ececec;
}

.list_table_btn2:hover {
	text-decoration: none;
	background: #ededed;
}


/*---表单页面--*/

.form_table {
	font-size: 14px;
}

.form_table td {
	padding: 5px 2px;
	vertical-align: top;
}

.form_table .title {
	text-align: right;
	font-weight: bold;
	line-height: 26px;
}

.form_textarea {
	border: 1px solid #cccccc;
	background: #ffffff;
}

.form_area {
	position: relative;
	padding-bottom: 40px;
}

.form_btn_area {
	position: fixed;
	bottom: 0;
	height: 50px;
	background: #f4f4f4;
	width: 100%;
	padding-top: 10px;
}

.form_btn_area_center {
	width: 980px;
	margin: 0 auto;
	text-align: right;
}

.form_btn_area_btn {
	display: inline-block;
	margin-right: 20px;
	padding: 5px 15px;
	line-height: 16px;
	font-size: 14px;
	color: #ffffff
}

a.form_btn_area_btn {
	color: #ffffff
}

.form_btn_area_btn:hover {
	text-decoration: none;
	color: #ffff00;
}

.form_btn_orange {
	background: #ec9600;
	border: 1px solid #d08400;
}

.form_btn_green {
	background: #42a23a;
	border: 1px solid #1e621c;
}

.form_btn_blue {
	background: #3a7ca2;
	border: 1px solid #155c85;
}

.form_btn_grey {
	background: #6b828a;
	border: 1px solid #4c5a64;
}


/*-----TAB界面-----*/

.zfw_tab {
	padding: 10px;
}

.zfw_tab_ul {
	position: relative;
	overflow: hidden;
	background: url(../images/zfw_tab_line.png) repeat-x;
	height: 36px;
}

.zfw_tab_ul li {
	float: left;
	padding-left: 20px;
	padding-right: 20px;
	margin-left: 10px;
	font-size: 14px;
}

.zfw_tab_ul .hover {
	border-left: 1px solid #ccc;
	border-right: 1px solid #ccc;
	border-top: 3px solid #b62221;
	height: 34px;
	line-height: 34px;
	background: #FFF;
	color: #b62221;
	font-weight: bold;
}

.zfw_tab_ul .none {
	border: 1px solid #ccc;
	height: 34px;
	line-height: 34px;
	background: #f7f7f7;
}

.zfw_center_block {
	background: #f5f5f5;
	border: 1px solid #e6e6e6;
	border-radius: 5px;
	padding: 10px 30px 10px 30px;
	margin: 0 auto;
	font-size: 14px;
	line-height: 35px;
}

.zfw_input_redline {
	border-bottom: 1px solid #F00;
	border-left: none;
	border-right: none;
	border-top: none;
	background: none;
	text-align: center;
	font-size: 14px;
	color: #F00;
	font-weight: bold;
}

.zfw_btn02 {
	background: #dd3c3a;
	font-size: 14px;
	color: #FFF;
	height: 35px;
	line-height: 35px;
	border-radius: 5px;
	border: none;
	cursor: pointer;
}

.zfw_btn02:hover {
	background: #c22d2b
}


/*-----日历界面-----*/

.rl_head {
	height: 52px;
	line-height: 52px;
	background: #eff5fe;
	font-size: 16px;
	border-bottom: 1px solid #dcdcdc;
}

.rl_btn_green a {
	border-radius: 3px;
	border: 1px solid #3a9c3b;
	background: #5cb45c;
	color: #ffffff;
	font-size: 12px;
	padding: 8px 10px;
	display: inline-block;
	line-height: 14px;
}

.rl_btn_green a:hover {
	color: #ffffff;
	background: #266f26;
	text-decoration: none;
}

.rl_btn_fy {
	margin-left: 10px;
	margin-right: 10px;
	font-size: 22px;
}

.rl_btn_fy a {
	border: 1px solid #eff5fe;
	border-radius: 3px;
	display: inline-block;
	padding: 8px 10px;
	line-height: 14px;
}

.rl_btn_fy a:hover {
	border: 1px solid #cccccc;
	text-decoration: none;
	background: #ffffff;
}

.rl_date {
	font-size: 22px;
	font-weight: bold;
}

.rl_btn_group {
	float: left;
	margin-right: 10px;
	*margin-top: 10px;
}

.rl_btn {
	float: left;
}

.rl_btn_group .rl_btn a {
	border: 1px solid #c2c2c2;
	border-right: none;
	background: url(../images/rl_btn.png) repeat-x #e2e2e2;
	line-height: 14px;
	padding: 8px 20px;
	font-size: 12px;
	display: inline-block;
}

.rl_btn_group .rl_btn a:hover {
	text-decoration: none;
	color: #ffffff;
	background: #49aff7;
	border: 1px solid #197fb3;
}

.rl_btn_group .first a {
	border-radius: 3px 0 0 3px;
	float: left;
}

.rl_btn_group .last a {
	border-radius: 0 3px 3px 0;
	float: left;
	border-right: 1px solid #c2c2c2
}

.rl_btn_group .active a {
	color: #ffffff;
	background: #49aff7;
	border: 1px solid #197fb3;
}

.rl_btn2 a {
	border-radius: 3px;
	border: 1px solid #c2c2c2;
	background: url(../images/rl_btn.png) repeat-x #e2e2e2;
	line-height: 14px;
	padding: 8px 20px;
	font-size: 12px;
	display: inline-block;
	_margin-top: 10px;
}

.rl_btn2 a:hover {
	text-decoration: none;
	color: #ffffff;
	background: #49aff7;
	border: 1px solid #197fb3;
}

#rl_main {
	width: 100%
}

#rl_side {
	width: 220px;
	float: left;
	overflow: hidden;
	text-align: center;
}

#rl_right {
	margin-left: 220px;
	overflow: hidden
}

.rl_left_color {
	height: 111px;
	line-height: 111px;
	width: 111px;
	font-size: 76px;
	color: #ffffff;
	display: inline-block;
	background: #49aff7;
	border-radius: 5px;
}

.rl_left_small {
	font-size: 12px;
	line-height: 18px;
}

.rl_left_date {
	font-size: 14px;
	line-height: 40px;
}

.rl_js {
	margin-top: 20px;
}

.rl_ri_table {
	background: #ffffff;
	border-left: 1px solid #e1e1e1;
	border-top: 1px solid #e1e1e1;
	border-right: 1px solid #e1e1e1;
}

.rl_ri_table td {
	border-bottom: 1px solid #e1e1e1;
	padding: 2px;
}

.rl_ri_table .time {
	text-align: center;
}

.rl_ri_table .add {
	text-align: center
}

.rl_ri_table .add a {
	color: #999;
}

.rl_table_richeng {
	background: #e1f5ff;
	border: 1px solid #7cb5d0;
	border-radius: 3px;
	padding: 1px 5px;
	margin-left: 5px;
	margin-right: 5px;
}

.rl_table_big td {
	height: 60px;
}

.rl_yue_table {
	background: #ffffff;
	border-left: 1px solid #e1e1e1;
	border-top: 1px solid #e1e1e1;
}

.rl_yue_table td {
	border-right: 1px solid #e1e1e1;
	border-bottom: 1px solid #e1e1e1;
	vertical-align: top;
}

.rl_yue_table .title {
	text-align: center;
	background: #f5f5f5;
	font-size: 16px;
	font-weight: bold;
	height: 30px;
}

.rl_yue_table .block_date {
	clear: both;
	overflow: hidden;
	padding-left: 5px;
	padding-right: 5px;
	height: 30px;
	line-height: 30px;
	font-size: 14px;
}

.rl_yue_table .block_date .fr {
	color: #999;
}

.rl_yue_table .rl_table_richeng {
	width: 97%;
	overflow: hidden;
	height: 20px;
	line-height: 20px;
	padding: 0;
	display: block;
	margin: 0px auto 5px auto;
}

.rl_yue_table .today {
	background: #fdeaa6;
}

.rl_window {
	border: 2px solid #cfcfcf;
	background: #f5f5f5;
	font-size: 14px;
}

.rl_window_title {
	background: url(../images/rl_btn.png) repeat-x #e2e2e2;
	height: 40px;
	line-height: 40px;
}

.rl_window_title .close {
	border-left: 1px solid #e2e2e2;
	text-align: center;
}

.rl_window_title .close a {
	display: block;
	height: 35px;
	padding-top: 5px;
	width: 40px;
}

.rl_window_title .close a:hover {
	background: #eeeeee;
}

.rl_window_title .name {
	font-size: 16px;
	font-weight: bold;
	padding-left: 10px;
	padding-right: 10px;
}

.rl_window_title .action a {
	width: 90px;
	text-align: center;
	display: block;
	font-size: 14px;
}

.rl_window_title .action a:hover {
	text-decoration: none;
	background: #eeeeee;
}

.rl_window_main {
	clear: both;
	padding: 15px;
}

.rl_window_table {
	background: #ffffff;
	border-radius: 5px;
	border: 1px solid #e4e4e4;
}

.rl_window_table .title {
	text-align: right;
}

.rl_window_table td {
	padding: 5px 3px;
	border-bottom: 1px solid #e4e4e4;
	line-height: 27px;
	vertical-align: top;
}

.rl_window_table .last td {
	border-bottom: none;
}

.rl_window_table .form_input {
	border-color: #e4e4e4;
}

.rl_window_table .form_textarea {
	border-color: #e4e4e4;
}

.rl_window_main .zhuyi {
	color: #666666;
	line-height: 20px;
	padding-top: 5px;
	padding-bottom: 10px;
}

.rl_window_table .area_xuanze {
	margin-left: 38px;
	font-size: 12px;
	color: #666666;
}

.rl_table_jiantou {
	position: relative;
	margin-top: 10px;
}

.table_jiantou {
	position: absolute;
	top: -18px;
	*top: -14px;
	left: 30px;
}


/*---表单页面17-11-16--*/

.formpage_bj {
	background: url(../images/index_bj.jpg) repeat;
}

.formpage_area {
	position: relative;
	width: 960px;
	margin: 10px auto 10px auto;
	background: #ffffff;
	min-height: 400px;
	border: 1px solid #e8e8e8;
}

.formpage_area_title {
	text-align: center;
	font-size: 20px;
	font-weight: bold;
	background: #fafafa;
	padding-top: 20px;
	padding-bottom: 20px;
	border-bottom: 1px solid #e8e8e8;
}

.formpage_area .main {
	padding: 20px;
	padding-top: 10px;
}

.form_block_title {
	background: #f4f4f4;
	font-size: 16px;
	font-weight: bold;
	border: 1px solid #e8e8e8;
	border-left: 3px solid #0073aa;
	padding-left: 10px;
	padding-top: 8px;
	padding-bottom: 8px;
	margin-bottom: 10px;
}

.formpage_table {
	font-size: 14px;
	margin-bottom: 10px;
}

.formpage_table td {
	padding: 5px 2px;
	vertical-align: top;
}

.formpage_table .title {
	text-align: right;
	line-height: 26px;
}

.formpage_table .form_textarea {
	border: 1px solid #c5c5c5;
	background: #ffffff;
}

.form_btn_big {
	padding: 10px 25px;
}

.table_add {
	text-align: center;
	background: #f0f5ff;
	font-size: 12px;
}

.table_add td {
	border-right: 1px solid #ffffff;
	border-bottom: 1px solid #ffffff;
	padding: 5px 3px;
}

.table_add .title {
	background: #f4f4f4;
	font-weight: bold;
}

.table_add .form_input {
	height: 23px;
	line-height: 23px;
}


/*------表单2018-02-05-----*/

.formpage_table_underline td {
	border-bottom: 1px dashed #ddd;
	font-size: 14px;
	padding: 3px 3px;
}

.formpage_table_underline .title {
	text-align: right;
	font-weight: bold;
}

.formpage_title_input {
	font-size: 14px;
}

.formpage_title_input .name {
	font-size: 16px;
	font-weight: bold;
	color: #0073aa;
	padding-left: 20px;
	padding-right: 20px;
}

.formpage_right_btn_area {
	width: 140px;
	box-shadow: 2px 2px 5px #888888;
}

.formpage_right_btn_area ul {
	margin-bottom: 0;
}

.formpage_right_btn_area ul li a {
	display: block;
	width: 140px;
	font-size: 14px;
	font-weight: bold;
	padding-left: 20px;
	border-bottom: 1px solid #ddd;
	height: 50px;
	line-height: 50px;
	background: url(../images/right_btn_area_03.png) repeat-x #f3f3f3;
}

.formpage_right_btn_area ul li a:hover {
	text-decoration: none;
	background: #0073aa;
	color: #fff;
}

.block_title2 {
	border-bottom: 1px solid #0073aa;
}

.block_title2 .name {
	display: inline-block;
	padding-left: 20px;
	padding-right: 20px;
	font-size: 16px;
	font-weight: bold;
	color: #0073aa;
}


/*----Tab标签(3月7日)-----*/

.block_title .nav {
	background: none;
	min-width: auto;
}

.block_title .nav .active {
	background: none;
}

.block_title .block_tab li {
	height: 40px;
	line-height: 40px;
	float: left;
	font-size: 16px;
}

.block_title .block_tab {
	border: none;
	font-size: 16px;
}

.block_title .block_tab li.active {
	font-weight: bold;
	border-bottom: 3px solid #0073aa
}

.block_title .block_tab li.active a {
	color: #0073aa;
}

.block_title .block_tab li a {
	border: none;
	background: none;
}

.block_title .block_tab li a:hover {
	border: none;
	background: none;
}

.block_tab>li.active>a,
.block_tab>li.active>a:hover,
.block_tab>li.active>a:focus {
	border: none;
	background: none;
}


/*-----3-12选择人员样式----*/

.width750 .modal-dialog {
	width: 750px;
}

.width750 .modal-header {
	background: #f4f4f4;
}

.width750 .modal-content {
	border: 0;
	border-bottom-left-radius: 8px;
	border-bottom-right-radius: 8px;
}

.width750 .modal-footer {
	text-align: center;
	background: #f4f4f4;
}

.width750 .modal-footer .bigbtn {
	width: 100px;
}

