<body class="formpage_bj">
<div class="container-fluid formpage_area" style="min-height: 300px">
    <form class="form-horizontal" id="form">
        <input type="hidden" id="textId"/>
        <input type="hidden" id="id" name="id" value=""/>
        <input name="opertation" id="opertation" type="hidden" value=""/><!-- 操作方法 -->
        <!-- 基本信息 -->
        <div class="row">
            <div class="col-md-12 ">
                <div class="block_title">
                    <span class="name">基本信息</span>
                </div>
            </div>
        </div>
        <div class="row m-t-15">
            <div class="col-md-12">
                <div class="form-group">
                   <label for="" class="col-md-2 control-label">标题：</label>
                    <div class="col-md-10">
                        <input type="text" class="form-control" id="title" name="title" placeholder="标题">
                    </div>
                </div>
                <div class="form-group">
                      <label for="" class="col-md-2 control-label">姓名：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="name" name="name" placeholder="姓名">
                    </div>
                    <label for="" class="col-md-2 control-label">时间范围：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="startTime" name="startTime" placeholder="时间范围">
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="col-md-2 control-label">出津时长：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="timeLong" name="timeLong" placeholder="出津时长">
                    </div>
                    <label for="" class="col-md-2 control-label">出津地点：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="place" name="place" placeholder="出津地点">
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="col-md-2 control-label">乘坐工具：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" id="traffic" name="traffic" placeholder="乘坐工具">
                    </div>
                </div>
                <div class="form-group">
                    <label for="" class="col-md-2 control-label">备注:</label>
                    <div class="col-md-10">
                        <textarea id="remark" name="remark" class="form-control" rows="3"></textarea>
                    </div>
                </div>
            </div>
        </div>
        <!-- 基本信息结束 -->
    </form>
</div>
<!-- 按钮区 -->
<div class="formpage_right_btn_area">
    <ul>
        <li><a onclick="comitForm();"><span class="glyphicon glyphicon-floppy-disk" aria-hidden="true"></span> 保存</a>
        </li>
    </ul>
</div>
<!-- 按钮区结束 -->
<script src="/static/core/jquery/jquery-1.11.2.min.js"></script>
<script src="/static/core/bootstrap/js/bootstrap.min.js"></script>
<script src="/static/core/bootstrap-table/bootstrap-table.min.js"></script>
<script src="/static/core/bootstrap-table/locale/bootstrap-table-zh-CN.min.js"></script>
<script src="/static/core/bootstrapvalidator/dist/js/bootstrapValidator.min.js"></script>
<script src="/static/core/layer/layer.js"></script>
<script src="/static/js/common/url.js"></script>
<script src="/static/js/common/mylayer.js"></script>

<script src="/static/core/laydate/laydate.js"></script>
<!-- 页面获取参数 -->
<script type="text/javascript" src="/common/js/getrequest.js"></script>
<script type="text/javascript" src="/common/js/commonFunction.js"></script>
<!-- 获取cookie值 -->
<script type="text/javascript" src="/product/workflow/js/common/getCook.js"></script>
<!--文件上传js-->
<!-- The jQuery UI widget factory, can be omitted if jQuery UI is already included -->
<script src="/static/core/jquery-fileupload/js/vendor/jquery.ui.widget.js"></script>
<!-- The Iframe Transport is required for browsers without support for XHR file uploads -->
<script src="/static/core/jquery-fileupload/js/jquery.iframe-transport.js"></script>
<!-- The basic File Upload plugin -->
<script src="/static/core/jquery-fileupload/js/jquery.fileupload.js"></script>
<script src="/static/js/common/myfilupload.js"></script>
<!--文件上传js end-->
<script src="/static/js/zhbg/ldtxb/jldcjbb/form.js"></script>

<!-- 引入自己模块功能的js -->
<script type="text/javascript" src="js/jldcjbbEdit.js"></script>
<script type="text/javascript">
    //绑定日期输入input
    laydate.render({
            elem: '#startTime' //指定元素
        , range: true
    });
</script>
</body>
</html>
