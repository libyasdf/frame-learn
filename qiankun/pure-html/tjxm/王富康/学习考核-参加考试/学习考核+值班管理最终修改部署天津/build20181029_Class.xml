<?xml version="1.0" encoding="UTF-8"?>
<project name="tjxm" default="default">
	<property name="export.dir.root" value="D:\导出项目文件\10.29.Class"></property>
	
	<target name="mqx">
		<mkdir dir="${export.dir.root}\tjxm"/>
		<copy todir="${export.dir.root}\tjxm" overwrite="true">
			<fileset refid="fengchao"/>
		</copy>
	</target>
	
	
	<fileset id="fengchao" dir="../../../../">
			
		<!--值班管理，	1.表单表头不固定的BUG修改；
						2.普网，专网电话校验4位数字，及导入时对普网电话及专网电话的校验修改；
						3.导出文件标头公网电话改为专网电话
						4.导入时普网专网电话都错误的情况下，只显示专网电话
						5.自动生成下月基础配置报错
						6.没有基础配置的时候查询基础配置的单位是报错，导致值班查询和总值班查询显示查询数据列表错误原因修改
		-->
		<!--后台-->
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/zbgl/dutytable/"/>
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/zbgl/config/"/>
		<!--页面-->
		<include name="WebContent/modules/zhbg/zbgl/dutytable/"/>
		<include name="WebContent/modules/zhbg/zbgl/shangbao/zbbReadOnlyForm.html"/>

		<!--基础配置-->
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/zbgl/config/"/>



		<!--学习考核  	1.IE浏览器考试完成后显示答案页面单选，多选，判断的单选和复选框勾选不会填的问题
						2.五分钟提示考试快要结束的弹框弹两次的问题
						3.考试时间到，自动交卷的提示问题
						4.学习考核-发送待办不成功
						5.6.发送两次待办的问题-->
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/xxkh/test/"/>
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/xxkh/testmanage/controller/"/>
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/xxkh/testmanage/source/"/>

		<include name="WebContent/modules/zhbg/xxkh/test/"/>
		<include name="WebContent/modules/zhbg/xxkh/testManage/ksglEnterTest.html"/>

		<!--公共的配置文件 -->
		<include name="config/spring/spring-context.xml"/>

	</fileset>
	
	<target name="all">
			<mkdir dir="${export.dir.root}/cqqx"/>
			<copy todir="${export.dir.root}/cqqx" overwrite="true">
				<fileset refid="fengchao"/>
			</copy>
		</target>
		<!--删除上次导出文件-->
		<target name="deleteOldFile" >  
		    <delete verbose="true" includeemptydirs="true" failonerror="false" >
		       <fileset dir="${export.dir.root}">
		           <include name="**/*"/>
		       </fileset>
		    </delete>
		</target> 

		<target name="default" depends="deleteOldFile,mqx">
	        <exec executable="cmd" failonerror="false">  
	            <arg line='/c explorer "${export.dir.root}\"' />  
	        </exec>
		</target>	
	</project>