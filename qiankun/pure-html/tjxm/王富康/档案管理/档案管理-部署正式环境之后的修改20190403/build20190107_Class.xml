<?xml version="1.0" encoding="UTF-8"?>
<project name="tjxm" default="default">
	<property name="export.dir.root" value="D:\导出项目文件\档案管理190403.Class"></property>
	
	<target name="mqx">
		<mkdir dir="${export.dir.root}\tjxm"/>
		<copy todir="${export.dir.root}\tjxm" overwrite="true">
			<fileset refid="fengchao"/>
		</copy>
	</target>
	
	
	<fileset id="fengchao" dir="../../../">

		<!--添加@Override-->
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/xxkh/paper/dao/XxkhPaperInfoDaoImpl.class"/>
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/xxkh/test/services/TestServiceImpl.class"/>
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/xxkh/testmanage/services/TestManageServiceImpl.class"/>
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/zbgl/config/services/ConfigServiceImpl.class"/>
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/zbgl/dutytable/services/DutyTableServiceImpl.class"/>
		<include name="build/classes/com/sinosoft/sinoep/modules/zhbg/zbgl/shangbao/services/ShangBaoServiceImpl.class"/>

		<!--平台所有的ID都是uuid字符串，全部按照字符串处理-->
		<!--<include name="build/classes/com/sinosoft/sinoep/modules/dagl/wdgl/services/DaglReceiveFileServiceImpl.class"/>
		<include name="build/classes/com/sinosoft/sinoep/modules/dagl/wdgl/services/DaglSendFileServiceImpl.class"/>-->

	</fileset>
	
	<target name="all">
			<mkdir dir="${export.dir.root}/cqqx"/>
			<copy todir="${export.dir.root}/cqqx" overwrite="true">
				<fileset refid="fengchao"/>
			</copy>
		</target>
		<!--删除上次导出文件-->
		<target name="deleteOldFile" >  
		    <delete verbose="true" includeemptydirs="true" failonerror="false" >
		       <fileset dir="${export.dir.root}">
		           <include name="**/*"/>
		       </fileset>
		    </delete>
		</target> 

		<target name="default" depends="deleteOldFile,mqx">
	        <exec executable="cmd" failonerror="false">  
	            <arg line='/c explorer "${export.dir.root}\"' />  
	        </exec>
		</target>	
	</project>